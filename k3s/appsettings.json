{
  "apiVersion": "v1",
  "kind": "ConfigMap",
  "metadata": {
    "name": "coinbase-websocket-config"
  },
  "data": {
    "appsettings.json": {
      "apiKey": "your-coinbase-api-key",
      "privateKey": "your-coinbase-private-key",
      "webSocketUrl": "wss://advanced-trade-ws.coinbase.com",
      "productIds": ["BTC-USD", "ETH-USD"],
      "channel": "candles",
      "kafkaBootstrapServers": "kafka-broker-0.kafka-broker-headless.kafka.svc.cluster.local:9092,kafka-broker-1.kafka-broker-headless.kafka.svc.cluster.local:9092,kafka-broker-2.kafka-broker-headless.kafka.svc.cluster.local:9092",
      "kafkaTopic": "coinbase-data",
      "kafkaUsername": "user1"
    }
  }
}