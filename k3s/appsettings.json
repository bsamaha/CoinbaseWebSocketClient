{
  "apiVersion": "v1",
  "kind": "ConfigMap",
  "metadata": {
    "name": "coinbase-websocket-appsettings"
  },
  "data": {
    "appsettings.json": "{\"apiKey\":\"${COINBASE_API_KEY}\",\"privateKey\":\"${COINBASE_PRIVATE_KEY}\",\"webSocketUrl\":\"${COINBASE_WEBSOCKET_URL}\",\"productIds\":[\"BTC-USD\",\"ETH-USD\"],\"channel\":\"${COINBASE_CHANNEL}\",\"webSocketBufferSize\":${WEBSOCKET_BUFFER_SIZE},\"kafkaBootstrapServers\":\"${KAFKA_BOOTSTRAP_SERVERS}\",\"kafkaTopic\":\"${KAFKA_TOPIC}\",\"kafkaUsername\":\"${KAFKA_USERNAME}\",\"kafkaPassword\":\"${KAFKA_PASSWORD}\",\"kafkaDebug\":\"${KAFKA_DEBUG}\"}"
  }
}